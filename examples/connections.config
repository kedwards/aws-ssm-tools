# AWS SSM Connections Configuration
# Format: INI-style config file with connection sections
# Each section defines a port forwarding configuration

[ReportBrowser-io]
profile = io
region = us-west-2
name = Report
host = localhost
port = 4444
local_port = 4444
url = http://localhost:4444

[ReportBrowser-how]
profile = how
region = us-west-2
name = Report
host = localhost
port = 4444
local_port = 4444
url = http://localhost:4444

[AlertManager]
profile = ps
region = us-west-2
name = Monitoring
host = localhost
port = 8428
local_port = 8428

[Vicoria Metrics]
profile = ps
region = us-west-2
name = Monitoring
host = localhost
port = 9093
local_port = 9093
